{
    "name": "mallet-zn/yii2-recaptcha-widget-php8.4",
    "description": "Yii2 Google reCAPTCHA v2 and v3 widget",
    "keywords": ["yii2", "captcha", "recaptcha", "google", "widget", "php8.4"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/mallet-zn/yii2-recaptcha-widget-php8.4",
        "issues": "https://github.com/mallet-zn/yii2-recaptcha-widget-php8.4/issues"
    },
    "authors": [
        {
            "name": "Matheus Mallet",
            "homepage": "https://github.com/mallet-zn/"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.11",
        "yiisoft/yii2-httpclient": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.34"
    },
    "autoload": {
        "psr-4": {
            "himiklab\\yii2\\recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "himiklab\\yii2\\recaptcha\\tests\\": "tests/"
        }
    }
}
